<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.qc.rc.dao.UserMapper">
	
    <select id="findRecord" resultType="com.qc.rc.entity.RechargeRecord">
<!--     select * from RC_RECHARGE_RECORD -->   
  		
  		select RR.* from RC_RECHARGE_RECORD RR,RC_USER RCU where RR.RR_USER_ID=RCU.USER_ID	
 
 	</select>
 	
	<delete id="deleteRecharge" parameterType="Integer">
	
		delete from RC_RECHARGE_RECORD where RR_ID=#{rrId}
	</delete> 

</mapper>